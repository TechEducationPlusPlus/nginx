    # TECHEDU.cf
	include /php_web/nginx/cf/chat;
	include /php_web/nginx/cf/judgata;
	include /php_web/nginx/cf/infocourse;
    
	server 
	{
        listen       80;
        server_name  admin.techedu.cf;
		
		root /php_web/Admin-Panel/;
		index index.html;	

		location / {
			proxy_pass http://127.0.0.1:10o;
		}
    }


    server 
	{
        listen       80;
        server_name  fb.techedu.cf;
		
		root /php_web/Facebook/;
		index index.html;	

		location / {
			proxy_pass http://127.0.0.1:101;
		}
    }

    server 
	{
        listen       80;
        server_name  resources.techedu.cf;
		
		root /php_web/Resources/;
		index index.html;	

		location / {
			try_files   $uri $uri/index.html $uri.html  =404;
		}
		location ~*  \.(jpg|jpeg|png|gif|ico|css|js|pdf)$ {
			expires 365d;
		}
    }
    server 
	{
        listen       80;
        server_name  techedu.cf;
		return 301 https://techedu.cf$request_uri;
    }

    # HTTPS server
    server 
	{
        listen       443 ssl;
        server_name  techedu.cf;

        ssl_certificate      /etc/letsencrypt/live/techedu.cf/cert.pem;
        ssl_certificate_key  /etc/letsencrypt/live/techedu.cf/privkey.pem;

        ssl_session_cache    shared:SSL:1m;
        ssl_session_timeout  5m;

        ssl_ciphers  HIGH:!aNULL:!MD5;
        ssl_prefer_server_ciphers  on;
	
		root /etc/www/TechEdu;
		index index.html index.htm;	

		location / {
			try_files   $uri $uri/ $uri.html  =404;
		}
		
		location ~*  \.(jpg|jpeg|png|gif|ico|css|js|pdf)$ {
			expires 365d;
		}
    }
